<body>
    <!-- Left Panel -->



    <div class="breadcrumbs">
        <div class="breadcrumbs-inner">
            <div class="row m-0">
                <div class="col-sm-4">
                    <div class="page-header float-left">
                        <div class="page-title">
                            <h1>Tablero</h1>
                        </div>
                    </div>
                </div>
                <div class="col-sm-8">
                    <div class="page-header float-right">
                        <div class="page-title">
                            <ol class="breadcrumb text-right">
                                <li><a href="#">Tablero</a></li>
                                <li><a href="#">Menú</a></li>
                                <li class="active">Ver Resultados</li>
                            </ol>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="content">
        <div class="animated fadeIn">

            <div class="ui-typography">
                <div class="row">
                    <div id="box-citasPr" class="col-md-12">
                        <div class="card">
                            <div class="card-header">
                                <strong class="card-title">Resultado de Examenes</strong>
                            </div>
                            <div class="card-body">
                                <table id="bootstrap-data-table" class="table table-striped table-bordered">
                                    <thead>
                                        <tr>
                                            <th>Nombre</th>
                                            <th>Cedula</th>
                                            <th>Fecha de Resultados</th>
                                            <th>Resultados de la cita</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td>Maria Delgado</td>
                                            <td>6722656</td>
                                            <td>2021-3-21</td>
                                            <td>
                                                <button id="btn-resultados" type="button" class="btn btn-info">Ver Detalles de Resultados</button>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="content">
                <div id="box-resultados" hidden class="col-lg-12">
                    <button id="show-citasPr" type="button" class="btn btn-info">Lista de Resultados</button>
                    <br>
                    <br>
                    <div class="card">

                        <div class="card-header">
                            <div id="card-patient" class="card">
                                <div class="card-body">
                                    <h5 class="card-title">Paciente: <td>Maria Delgado</td></h5>
                                    <hr>
                                    <p class="card-text">Cedula: <td>6722656</td></p>
                                    <p class="card-text">Factura: <td>232355</td></p>
                                    <p class="card-text">F/N: <td>ABCDF</td></p>
                                    <p class="card-text">Edad: <td>23</td></p>
                                    <p class="card-text">Emisión: <td>ABCD</td></p>

                                </div>
                            </div>
                            <strong class="card-title">Resultados</strong>
                        </div>
                        <div class="card-body">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th scope="col">Caracteristica</th>
                                        <th scope="col">Resultado</th>
                                        <th scope="col">Inferior</th>
                                        <th scope="col">Superior</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <th scope="row">Hemoglobina</th>
                                        <td>14.0</td>
                                        <td>11.5</td>
                                        <td>15.0</td>
                                    </tr>

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div><!-- .animated -->
            </div><!-- .content -->




        </div><!-- .animated -->
    </div><!-- .content -->
